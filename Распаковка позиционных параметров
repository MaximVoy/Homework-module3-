def print_params(a = 1, b = 'строка', c = True):
    print(a, b, c)



values_list = [11.11, True, (15, 'cat')]
values_dict = {'a' : 22, 'b': 'dog', 'c' : 55.12}
values_list_2 = [54.32, 'Строка' ]




print_params()
print_params(3, 'дом')
print_params(False)
print_params(b = 25)
print_params(c = [1,2,3])
print_params(*values_list)
print_params(**values_dict)
print_params(*values_list_2, 42)
# Надо оставить все требуемые проверки или только последнюю строку вызова?
